{
  "name": "example-sanity-studio",
  "private": true,
  "version": "1.0.0",
  "engineStrict": true,
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky",
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "check:lint": "eslint . --ext .js,.cjs,.mjs,.ts  --max-warnings 0",
    "check:fmt": "prettier -c \"**/*.{js,cjs,mjs,ts,json,md,yml}\"",
    "check:types": "tsc -p tsconfig.json --noEmit",
    "check:all": "concurrently -g --timings \"npm:check:lint\" \"npm:check:fmt\" \"npm:check:types\""
  },
  "dependencies": {
    "@sanity/assist": "4.4.7",
    "@sanity/document-internationalization": "4.0.0",
    "@sanity/ui": "3.0.6",
    "@sanity/vision": "4.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sanity": "4.3.0",
    "sanity-plugin-internationalized-array": "3.1.4",
    "sanity-plugin-media": "4.0.0",
    "valibot": "1.0.0"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "5.0.2",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/react": "19.1.2",
    "concurrently": "9.1.2",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "prettier": "3.6.2",
    "typescript": "5.8.3"
  }
}
